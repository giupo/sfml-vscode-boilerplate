		// "bin/": true,
		// "build/": true,
		"**/*.7z": true,
		"**/*.rar": true,
		"**/Thumbs.db": true
	},
	"files.associations": {
		"*.json": "jsonc",
		"*.stats": "cpp",
		"*.desktop": "properties",
		".clang-format": "yaml"
	},
	"terminal.integrated.automationShell.windows": "C:/Program Files/Git/bin/bash.exe",
