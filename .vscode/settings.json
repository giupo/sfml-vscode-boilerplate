		"activityBarBadge.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#6f9e33",
		"titleBar.activeBackground": "#8ac242",
		"titleBar.activeForeground": "#15202b",
		"titleBar.inactiveBackground": "#8ac24299",
		"titleBar.inactiveForeground": "#15202b99"
	},
	"[applescript]": {
		"files.encoding": "utf8"
